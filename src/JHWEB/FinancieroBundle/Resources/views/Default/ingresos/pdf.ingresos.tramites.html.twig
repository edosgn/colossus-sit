<html>
    <header>
        <meta charset="utf-8">
    </header>
    <body>
        <br>
        {% if pagadas %}
            <h3>PAGADAS</h3>
            <table border="1" style="text-align:center;">
                <tr>
                    <td><b> COD </b></td>
                    <td><b> Tramites </b></td>
                    <td><b> Cantidad </b></td>
                    <td><b> Valor </b></td>
                    <td><b> Total </b></td>
                </tr>
                {% for tramite in arrayTramites %}
                    <tr>
                        <td>{{ tramite.id }}</td>
                        <td>{{ tramite.nombre }}</td>
                        <td>{{ tramite.cantidad }}</td>
                        <td>{{ tramite.valor }}</td>
                        <td>{{ tramite.total2 }}</td>
                    </tr>
                {% endfor %}
                <tr>
                    <td style="float:left;" colspan="4"><b>TOTAL INGRESOS</b></td>
                    <td>{{ totalTramites }}</td>
                </tr>
            </table>
        {% endif %}
        <br>
        <h3>CONCEPTOS<h3>
        <table border="1" style="text-align:center;">
            <tr>
                <td><b>Id</b></td>
                <td><b>Nombre</b></td>
                <td><b>Cantidad</b></td>
                <td><b>Valor</b></td>
                <td><b>Total</b></td>
            </tr>
            {% for concepto in arrayConceptos %}
                    <tr>
                        <td>{{ concepto.id }}</td>
                        <td>{{ concepto.nombre }}</td>
                        <td>{{ concepto.cantidad}}</td>
                        <td>{{ concepto.valor }}</td>
                        <td>{{ concepto.total }}</td>
                    </tr>
            {% endfor %}
            <tr>
                <td style="float:left;" colspan="4"><b>TOTAL CONCEPTOS</b></td>
                <td>{{ totalConceptos }}</td>
            </tr>
            <tr>
                <td style="float:left;" colspan="4"><b>TOTAL INGRESOS SUBDETRA</b></td>
                <td>{{ totalTramites + totalConceptos }}</td>
            </tr>
        </table>
        <br>
        <table border="1" style="text-align:center;">
            <tr>
                <td><b> RBO CAJA ANULADO </b></td>
                <td>{{ cantAnuladas }}</td>
            </tr>
            <tr>
                <td><b> RBO RETEFUENTE ANULADO </b></td>
                <td><b> {{ cantTraspasos }} </b></td>
            </tr>
        </table>
        <br>
        <br>
        <table border="1" style="text-align:center;">
            <tr>
                <td><b>Relación No. de recibos</b></td>
                <td><b>Desde</b></td>
                <td><b>Hasta</b></td>
            </tr>
            <tr>
                <td>Recibos de trámites</td>
                <td>{{ min }}</td>
                <td>{{ max }}</td>
            </tr>
            <tr>
                <td>Recibos de rte. fte.</td>
                <td>{{ minAnulados }}</td>
                <td>{{ maxAnulados }}</td>
            </tr>
        </table>
        <br>
        <br>
        {# <table border="1" style="text-align:center;">
                <tr>
                    <td><b> Sustratos CDA </b></td>
                    <td><b> Cantidad </b></td>
                    <td><b> Valor </b></td>
                    <td><b> Total </b></td>
                </tr>
            {% for insumo in insumos %}
                    <tr>
                        <td>{{ insumo.categoria }}</td>
                        <td>cantidad</td>
                        <td>{{ insumo.tipo.nombre }}</td>
                        <td>total</td>
                    </tr>
                {% endfor %}
            </table> #}
        {# {% if anuladas %}
            <h3>ANULADAS</h3>
            <table border="1" style="text-align:center;">
                <tr>
                    <td><b> COD </b></td>
                    <td><b> Concepto </b></td>
                    <td><b> Cantidad </b></td>
                    <td><b> Valor </b></td>
                    <td><b> Total </b></td>
                </tr>
                {% for anulada in anuladas %}
                    <tr>
                        <td>{{ anulada.tramiteFactura.precio.tramite.codigo }}</td>
                        <td>{{ anulada.tramiteFactura.precio.tramite.nombre }}</td>
                        <td>{{ cantAnuladas }}</td>
                        <td>{{ anulada.tramiteFactura.precio.valor }}</td>
                        <td>{{ cantAnuladas * anulada.tramiteFactura.precio.valor }}</td>
                    </tr>
                {% endfor %}
                <tr>
                    <td style="float:left;" colspan="4"><b>TOTAL INGRESOS</b></td>
                    <td>{{ valorTramitesAnulados }}</td>
                </tr>
            </table>
        {% endif %} #}
    </body>
</html>

