{% extends 'base.html.twig' %}

{% block body %}
    <h1>Representanteempresa</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ representanteEmpresa.id }}</td>
            </tr>
            <tr>
                <th>Fechainicial</th>
                <td>{% if representanteEmpresa.fechaInicial %}{{ representanteEmpresa.fechaInicial|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafinal</th>
                <td>{% if representanteEmpresa.fechaFinal %}{{ representanteEmpresa.fechaFinal|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{% if representanteEmpresa.estado %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('representanteempresa_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('representanteempresa_edit', { 'id': representanteEmpresa.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
