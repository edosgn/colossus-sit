{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgradioaccion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgRadioAccion.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgRadioAccion.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgRadioAccion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgradioaccion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgradioaccion_edit', { 'id': cfgRadioAccion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
