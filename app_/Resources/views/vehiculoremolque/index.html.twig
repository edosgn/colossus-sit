                <td>{{ vehiculoRemolque.cargaUtilMaxima }}</td>
                <td>{{ vehiculoRemolque.rodaje }}</td>
                <td>{{ vehiculoRemolque.numeroEjes }}</td>
                <td>{{ vehiculoRemolque.numeroLlantas }}</td>
                <td>{{ vehiculoRemolque.fichaTecnica }}</td>
                <td>{{ vehiculoRemolque.altoTotal }}</td>
                <td>{{ vehiculoRemolque.largoTotal }}</td>
                <td>{{ vehiculoRemolque.anchoTotal }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('vehiculoremolque_show', { 'id': vehiculoRemolque.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('vehiculoremolque_edit', { 'id': vehiculoRemolque.id }) }}">edit</a>
