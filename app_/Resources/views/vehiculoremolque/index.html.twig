{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vehiculoremolques list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Pesobrutovehiculo</th>
                <th>Cargautilmaxima</th>
                <th>Rodaje</th>
                <th>Numeroejes</th>
                <th>Numerollantas</th>
                <th>Fichatecnica</th>
