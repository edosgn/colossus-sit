{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgauditorium</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgAuditorium.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ cfgAuditorium.url }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ cfgAuditorium.token }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if cfgAuditorium.fecha %}{{ cfgAuditorium.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datos</th>
                <td>{% if cfgAuditorium.datos %}{{ cfgAuditorium.datos|join(', ') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Tipopeticion</th>
                <td>{{ cfgAuditorium.tipoPeticion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgauditoria_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgauditoria_edit', { 'id': cfgAuditorium.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
