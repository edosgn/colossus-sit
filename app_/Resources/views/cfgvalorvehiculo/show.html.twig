{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgvalorvehiculo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgValorVehiculo.id }}</td>
            </tr>
            <tr>
                <th>Cilindraje</th>
                <td>{{ cfgValorVehiculo.cilindraje }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if cfgValorVehiculo.fecha %}{{ cfgValorVehiculo.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ cfgValorVehiculo.valor }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgvalorvehiculo_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
