{% extends 'base.html.twig' %}

{% block body %}
    <h1>Msvrevision</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ msvRevision.id }}</td>
            </tr>
            <tr>
                <th>Fecharecepcion</th>
                <td>{% if msvRevision.fechaRecepcion %}{{ msvRevision.fechaRecepcion|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechadevolucion</th>
                <td>{% if msvRevision.fechaDevolucion %}{{ msvRevision.fechaDevolucion|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechaotorgamiento</th>
                <td>{% if msvRevision.fechaOtorgamiento %}{{ msvRevision.fechaOtorgamiento|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Funcionario</th>
                <td>{{ msvRevision.funcionario }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('msvrevision_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('msvrevision_edit', { 'id': msvRevision.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
