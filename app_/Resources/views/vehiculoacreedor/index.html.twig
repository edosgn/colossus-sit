{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vehiculoacreedors list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for vehiculoAcreedor in vehiculoAcreedors %}
            <tr>
                <td><a href="{{ path('vehiculoacreedor_show', { 'id': vehiculoAcreedor.id }) }}">{{ vehiculoAcreedor.id }}</a></td>
                <td>{% if vehiculoAcreedor.estado %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('vehiculoacreedor_show', { 'id': vehiculoAcreedor.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('vehiculoacreedor_edit', { 'id': vehiculoAcreedor.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vehiculoacreedor_new') }}">Create a new vehiculoAcreedor</a>
        </li>
    </ul>
{% endblock %}
