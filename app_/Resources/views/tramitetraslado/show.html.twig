{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tramitetraslado</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tramiteTraslado.id }}</td>
            </tr>
            <tr>
                <th>Fechasalida</th>
                <td>{% if tramiteTraslado.fechaSalida %}{{ tramiteTraslado.fechaSalida|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Numeroguia</th>
                <td>{{ tramiteTraslado.numeroGuia }}</td>
            </tr>
            <tr>
                <th>Nombreempresa</th>
                <td>{{ tramiteTraslado.nombreEmpresa }}</td>
            </tr>
            <tr>
                <th>Numerorut</th>
                <td>{{ tramiteTraslado.numeroRut }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tramitetraslado_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tramitetraslado_edit', { 'id': tramiteTraslado.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
