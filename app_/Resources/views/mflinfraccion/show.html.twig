{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mflinfraccion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mflInfraccion.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ mflInfraccion.nombre }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ mflInfraccion.codigo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ mflInfraccion.descripcion }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if mflInfraccion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('mflinfraccion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('mflinfraccion_edit', { 'id': mflInfraccion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
