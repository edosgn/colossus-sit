{% extends 'base.html.twig' %}

{% block body %}
    <h1>TramiteEspecifico</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tramiteEspecifico.id }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ tramiteEspecifico.valor }}</td>
            </tr>
            <tr>
                <th>Datos</th>
                <td>{% if tramiteEspecifico.datos %}{{ tramiteEspecifico.datos|join(', ') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tramiteespecifico_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tramiteespecifico_edit', { 'id': tramiteEspecifico.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
