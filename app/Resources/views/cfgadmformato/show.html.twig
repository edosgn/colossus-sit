{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgadmformato</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgAdmFormato.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgAdmFormato.nombre }}</td>
            </tr>
            <tr>
                <th>Cuerpo</th>
                <td>{{ cfgAdmFormato.cuerpo }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ cfgAdmFormato.codigo }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgAdmFormato.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgadmformato_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgadmformato_edit', { 'id': cfgAdmFormato.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
