{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgadmformatos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Cuerpo</th>
                <th>Codigo</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cfgAdmFormato in cfgAdmFormatos %}
            <tr>
                <td><a href="{{ path('cfgadmformato_show', { 'id': cfgAdmFormato.id }) }}">{{ cfgAdmFormato.id }}</a></td>
                <td>{{ cfgAdmFormato.nombre }}</td>
                <td>{{ cfgAdmFormato.cuerpo }}</td>
                <td>{{ cfgAdmFormato.codigo }}</td>
                <td>{% if cfgAdmFormato.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('cfgadmformato_show', { 'id': cfgAdmFormato.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('cfgadmformato_edit', { 'id': cfgAdmFormato.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgadmformato_new') }}">Create a new cfgAdmFormato</a>
        </li>
    </ul>
{% endblock %}
