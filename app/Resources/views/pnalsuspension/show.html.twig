{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalsuspension</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pnalSuspension.id }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if pnalSuspension.fechaInicio %}{{ pnalSuspension.fechaInicio|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{% if pnalSuspension.fechaFin %}{{ pnalSuspension.fechaFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ pnalSuspension.observacion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalsuspension_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pnalsuspension_edit', { 'id': pnalSuspension.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
