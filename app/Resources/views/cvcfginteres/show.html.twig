{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cvcfgintere</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cvCfgIntere.id }}</td>
            </tr>
            <tr>
                <th>Anio</th>
                <td>{{ cvCfgIntere.anio }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ cvCfgIntere.mes }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ cvCfgIntere.valor }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cvCfgIntere.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cvcfginteres_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cvcfginteres_edit', { 'id': cvCfgIntere.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
