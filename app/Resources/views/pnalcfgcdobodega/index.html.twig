{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalcfgcdobodegas list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Cantidadrecibida</th>
                <th>Cantidaddisponible</th>
                <th>Estado</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pnalCfgCdoBodega in pnalCfgCdoBodegas %}
            <tr>
                <td><a href="{{ path('pnalcfgcdobodega_show', { 'id': pnalCfgCdoBodega.id }) }}">{{ pnalCfgCdoBodega.id }}</a></td>
                <td>{% if pnalCfgCdoBodega.fecha %}{{ pnalCfgCdoBodega.fecha|date('Y-m-d') }}{% endif %}</td>
                <td>{{ pnalCfgCdoBodega.cantidadRecibida }}</td>
                <td>{{ pnalCfgCdoBodega.cantidadDisponible }}</td>
                <td>{{ pnalCfgCdoBodega.estado }}</td>
                <td>{% if pnalCfgCdoBodega.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('pnalcfgcdobodega_show', { 'id': pnalCfgCdoBodega.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('pnalcfgcdobodega_edit', { 'id': pnalCfgCdoBodega.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalcfgcdobodega_new') }}">Create a new pnalCfgCdoBodega</a>
        </li>
    </ul>
{% endblock %}
