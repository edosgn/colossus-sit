{% extends 'base.html.twig' %}

{% block body %}
    <h1>Frofactramite</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ froFacTramite.id }}</td>
            </tr>
            <tr>
                <th>Realizado</th>
                <td>{% if froFacTramite.realizado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if froFacTramite.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('frofactramite_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('frofactramite_edit', { 'id': froFacTramite.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
