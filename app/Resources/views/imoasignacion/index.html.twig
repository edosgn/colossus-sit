{% extends 'base.html.twig' %}

{% block body %}
    <h1>Imoasignacions list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for imoAsignacion in imoAsignacions %}
            <tr>
                <td><a href="{{ path('imoasignacion_show', { 'id': imoAsignacion.id }) }}">{{ imoAsignacion.id }}</a></td>
                <td>{% if imoAsignacion.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('imoasignacion_show', { 'id': imoAsignacion.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('imoasignacion_edit', { 'id': imoAsignacion.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imoasignacion_new') }}">Create a new imoAsignacion</a>
        </li>
    </ul>
{% endblock %}
