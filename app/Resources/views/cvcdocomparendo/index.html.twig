                <th>Polca</th>
                <th>Audiencia</th>
                <th>Recurso</th>
                <th>Notificado</th>
                <th>Pagado</th>
                <th>Curso</th>
                <th>Porcentajedescuento</th>
                <th>Activo</th>
                <th>Servicio</th>
                <th>Clase</th>
                <th>Radioaccion</th>
                <th>Modalidadtransporte</th>
                <th>Transportepasajero</th>
                <th>Transporteespecial</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cvCdoComparendo in cvCdoComparendos %}
            <tr>
                <td><a href="{{ path('cvcdocomparendo_show', { 'id': cvCdoComparendo.id }) }}">{{ cvCdoComparendo.id }}</a></td>
                <td>{% if cvCdoComparendo.fecha %}{{ cvCdoComparendo.fecha|date('Y-m-d') }}{% endif %}</td>
                <td>{% if cvCdoComparendo.hora %}{{ cvCdoComparendo.hora|date('H:i:s') }}{% endif %}</td>
                <td>{{ cvCdoComparendo.expediente }}</td>
                <td>{{ cvCdoComparendo.direccion }}</td>
                <td>{{ cvCdoComparendo.localidad }}</td>
                <td>{{ cvCdoComparendo.placa }}</td>
                <td>{{ cvCdoComparendo.infractorIdentificacion }}</td>
                <td>{{ cvCdoComparendo.infractorNumeroLicenciaConduccion }}</td>
                <td>{{ cvCdoComparendo.categoria }}</td>
                <td>{% if cvCdoComparendo.fechaExpedicion %}{{ cvCdoComparendo.fechaExpedicion|date('Y-m-d') }}{% endif %}</td>
                <td>{% if cvCdoComparendo.fechaVencimiento %}{{ cvCdoComparendo.fechaVencimiento|date('Y-m-d') }}{% endif %}</td>
                <td>{{ cvCdoComparendo.infractorNombres }}</td>
                <td>{{ cvCdoComparendo.infractorApellidos }}</td>
                <td>{{ cvCdoComparendo.infractorDireccion }}</td>
                <td>{% if cvCdoComparendo.infractorFechaNacimiento %}{{ cvCdoComparendo.infractorFechaNacimiento|date('Y-m-d') }}{% endif %}</td>
                <td>{{ cvCdoComparendo.infractorTelefono }}</td>
                <td>{{ cvCdoComparendo.infractorMunicipioResidencia }}</td>
                <td>{{ cvCdoComparendo.infractorEmail }}</td>
                <td>{{ cvCdoComparendo.numeroLicenciaTransito }}</td>
                <td>{{ cvCdoComparendo.propietarioIdentificacion }}</td>
                <td>{{ cvCdoComparendo.propietarioNombre }}</td>
                <td>{{ cvCdoComparendo.propietarioApellidos }}</td>
                <td>{{ cvCdoComparendo.empresaNit }}</td>
                <td>{{ cvCdoComparendo.empresaNombre }}</td>
                <td>{{ cvCdoComparendo.tarjetaOperacion }}</td>
                <td>{{ cvCdoComparendo.observacionesAgente }}</td>
                <td>{{ cvCdoComparendo.observacionesDigitador }}</td>
                <td>{{ cvCdoComparendo.testigoNombres }}</td>
                <td>{{ cvCdoComparendo.testigoApellidos }}</td>
                <td>{{ cvCdoComparendo.testigoIdentificacion }}</td>
                <td>{{ cvCdoComparendo.testigoDireccion }}</td>
                <td>{{ cvCdoComparendo.testigoTelefono }}</td>
                <td>{% if cvCdoComparendo.fuga %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.accidente %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.retencionLicencia %}Yes{% else %}No{% endif %}</td>
                <td>{{ cvCdoComparendo.valorInfraccion }}</td>
                <td>{{ cvCdoComparendo.interesMora }}</td>
                <td>{{ cvCdoComparendo.valorPagar }}</td>
                <td>{{ cvCdoComparendo.urlDocumento }}</td>
                <td>{% if cvCdoComparendo.polca %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.audiencia %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.recurso %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.notificado %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.pagado %}Yes{% else %}No{% endif %}</td>
                <td>{% if cvCdoComparendo.curso %}Yes{% else %}No{% endif %}</td>
                <td>{{ cvCdoComparendo.porcentajeDescuento }}</td>
                <td>{% if cvCdoComparendo.activo %}Yes{% else %}No{% endif %}</td>
                <td>{{ cvCdoComparendo.servicio }}</td>
                <td>{{ cvCdoComparendo.clase }}</td>
                <td>{{ cvCdoComparendo.radioAccion }}</td>
                <td>{{ cvCdoComparendo.modalidadTransporte }}</td>
                <td>{{ cvCdoComparendo.transportePasajero }}</td>
                <td>{{ cvCdoComparendo.transporteEspecial }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('cvcdocomparendo_show', { 'id': cvCdoComparendo.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('cvcdocomparendo_edit', { 'id': cvCdoComparendo.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cvcdocomparendo_new') }}">Create a new cvCdoComparendo</a>
        </li>
    </ul>
{% endblock %}
