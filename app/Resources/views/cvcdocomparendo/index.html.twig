{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cvcdocomparendos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Hora</th>
                <th>Expediente</th>
                <th>Direccion</th>
                <th>Localidad</th>
                <th>Placa</th>
                <th>Infractoridentificacion</th>
                <th>Infractornumerolicenciaconduccion</th>
                <th>Categoria</th>
                <th>Fechaexpedicion</th>
                <th>Fechavencimiento</th>
                <th>Infractornombres</th>
                <th>Infractorapellidos</th>
                <th>Infractordireccion</th>
                <th>Infractorfechanacimiento</th>
                <th>Infractortelefono</th>
                <th>Infractormunicipioresidencia</th>
                <th>Infractoremail</th>
                <th>Numerolicenciatransito</th>
                <th>Propietarioidentificacion</th>
                <th>Propietarionombre</th>
                <th>Propietarioapellidos</th>
                <th>Empresanit</th>
                <th>Empresanombre</th>
                <th>Tarjetaoperacion</th>
                <th>Observacionesagente</th>
                <th>Observacionesdigitador</th>
                <th>Testigonombres</th>
                <th>Testigoapellidos</th>
                <th>Testigoidentificacion</th>
                <th>Testigodireccion</th>
                <th>Testigotelefono</th>
                <th>Fuga</th>
                <th>Accidente</th>
                <th>Retencionlicencia</th>
