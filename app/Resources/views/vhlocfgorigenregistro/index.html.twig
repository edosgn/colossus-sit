{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhlocfgorigenregistros list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for vhloCfgOrigenRegistro in vhloCfgOrigenRegistros %}
            <tr>
                <td><a href="{{ path('vhlocfgorigenregistro_show', { 'id': vhloCfgOrigenRegistro.id }) }}">{{ vhloCfgOrigenRegistro.id }}</a></td>
                <td>{{ vhloCfgOrigenRegistro.nombre }}</td>
                <td>{% if vhloCfgOrigenRegistro.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('vhlocfgorigenregistro_show', { 'id': vhloCfgOrigenRegistro.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('vhlocfgorigenregistro_edit', { 'id': vhloCfgOrigenRegistro.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlocfgorigenregistro_new') }}">Create a new vhloCfgOrigenRegistro</a>
        </li>
    </ul>
{% endblock %}
