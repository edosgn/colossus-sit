{% extends 'base.html.twig' %}

{% block body %}
    <h1>Gdcfgseccions list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for gdCfgSeccion in gdCfgSeccions %}
            <tr>
                <td><a href="{{ path('gdcfgseccion_show', { 'id': gdCfgSeccion.id }) }}">{{ gdCfgSeccion.id }}</a></td>
                <td>{{ gdCfgSeccion.nombre }}</td>
                <td>{% if gdCfgSeccion.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('gdcfgseccion_show', { 'id': gdCfgSeccion.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('gdcfgseccion_edit', { 'id': gdCfgSeccion.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('gdcfgseccion_new') }}">Create a new gdCfgSeccion</a>
        </li>
    </ul>
{% endblock %}
