{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mflinfraccioncategorium</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mflInfraccionCategorium.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ mflInfraccionCategorium.nombre }}</td>
