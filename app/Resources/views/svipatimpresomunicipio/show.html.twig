{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svipatimpresomunicipio</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ svIpatImpresoMunicipio.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if svIpatImpresoMunicipio.fecha %}{{ svIpatImpresoMunicipio.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ svIpatImpresoMunicipio.cantidad }}</td>
            </tr>
            <tr>
                <th>Numeroacta</th>
                <td>{{ svIpatImpresoMunicipio.numeroActa }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if svIpatImpresoMunicipio.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svipatimpresomunicipio_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('svipatimpresomunicipio_edit', { 'id': svIpatImpresoMunicipio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
