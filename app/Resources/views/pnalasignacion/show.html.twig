{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalasignacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pnalAsignacion.id }}</td>
            </tr>
            <tr>
                <th>Desde</th>
                <td>{{ pnalAsignacion.desde }}</td>
            </tr>
            <tr>
                <th>Hasta</th>
                <td>{{ pnalAsignacion.hasta }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if pnalAsignacion.fecha %}{{ pnalAsignacion.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Rangos</th>
                <td>{{ pnalAsignacion.rangos }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if pnalAsignacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalasignacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pnalasignacion_edit', { 'id': pnalAsignacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
