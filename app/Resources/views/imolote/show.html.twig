{% extends 'base.html.twig' %}

{% block body %}
    <h1>Imolote</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imoLote.id }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ imoLote.estado }}</td>
            </tr>
            <tr>
                <th>Numeroacta</th>
                <td>{{ imoLote.numeroActa }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if imoLote.fecha %}{{ imoLote.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Rangoinicio</th>
                <td>{{ imoLote.rangoInicio }}</td>
            </tr>
            <tr>
                <th>Rangofin</th>
                <td>{{ imoLote.rangoFin }}</td>
            </tr>
            <tr>
                <th>Referencia</th>
                <td>{{ imoLote.referencia }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ imoLote.tipo }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ imoLote.cantidad }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imolote_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imolote_edit', { 'id': imoLote.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
