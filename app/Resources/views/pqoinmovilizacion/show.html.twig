{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pqoinmovilizacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pqoInmovilizacion.id }}</td>
            </tr>
            <tr>
                <th>Numero_comparendo</th>
                <td>{{ pqoInmovilizacion.numerocomparendo }}</td>
            </tr>
            <tr>
                <th>Placa</th>
                <td>{{ pqoInmovilizacion.placa }}</td>
            </tr>
            <tr>
                <th>Fechaingreso</th>
                <td>{% if pqoInmovilizacion.fechaIngreso %}{{ pqoInmovilizacion.fechaIngreso|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Horaingreso</th>
                <td>{% if pqoInmovilizacion.horaIngreso %}{{ pqoInmovilizacion.horaIngreso|date('H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechasalida</th>
                <td>{% if pqoInmovilizacion.fechaSalida %}{{ pqoInmovilizacion.fechaSalida|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Horasalida</th>
                <td>{% if pqoInmovilizacion.horaSalida %}{{ pqoInmovilizacion.horaSalida|date('H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Numerosalida</th>
                <td>{{ pqoInmovilizacion.numeroSalida }}</td>
            </tr>
            <tr>
                <th>Numeroinventario</th>
                <td>{{ pqoInmovilizacion.numeroInventario }}</td>
            </tr>
            <tr>
                <th>Dias</th>
                <td>{{ pqoInmovilizacion.dias }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ pqoInmovilizacion.observaciones }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ pqoInmovilizacion.valor }}</td>
            </tr>
            <tr>
                <th>Costotrayecto</th>
                <td>{{ pqoInmovilizacion.costoTrayecto }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ pqoInmovilizacion.estado }}</td>
            </tr>
            <tr>
                <th>Salida</th>
                <td>{% if pqoInmovilizacion.salida %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if pqoInmovilizacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pqoinmovilizacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pqoinmovilizacion_edit', { 'id': pqoInmovilizacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
