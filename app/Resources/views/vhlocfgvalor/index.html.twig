{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhlocfgvalors list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Cilindraje</th>
                <th>Anio</th>
                <th>Valor</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for vhloCfgValor in vhloCfgValors %}
            <tr>
                <td><a href="{{ path('vhlocfgvalor_show', { 'id': vhloCfgValor.id }) }}">{{ vhloCfgValor.id }}</a></td>
                <td>{{ vhloCfgValor.cilindraje }}</td>
                <td>{{ vhloCfgValor.anio }}</td>
                <td>{{ vhloCfgValor.valor }}</td>
                <td>{% if vhloCfgValor.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('vhlocfgvalor_show', { 'id': vhloCfgValor.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('vhlocfgvalor_edit', { 'id': vhloCfgValor.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlocfgvalor_new') }}">Create a new vhloCfgValor</a>
        </li>
    </ul>
{% endblock %}
