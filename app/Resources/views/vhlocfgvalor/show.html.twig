{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhlocfgvalor</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vhloCfgValor.id }}</td>
            </tr>
            <tr>
                <th>Cilindraje</th>
                <td>{{ vhloCfgValor.cilindraje }}</td>
            </tr>
            <tr>
                <th>Anio</th>
                <td>{{ vhloCfgValor.anio }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ vhloCfgValor.valor }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if vhloCfgValor.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlocfgvalor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vhlocfgvalor_edit', { 'id': vhloCfgValor.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
