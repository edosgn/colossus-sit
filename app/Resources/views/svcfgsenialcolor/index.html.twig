{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svcfgsenialcolors list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for svCfgSenialColor in svCfgSenialColors %}
            <tr>
                <td><a href="{{ path('svcfgsenialcolor_show', { 'id': svCfgSenialColor.id }) }}">{{ svCfgSenialColor.id }}</a></td>
                <td>{{ svCfgSenialColor.nombre }}</td>
                <td>{% if svCfgSenialColor.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('svcfgsenialcolor_show', { 'id': svCfgSenialColor.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('svcfgsenialcolor_edit', { 'id': svCfgSenialColor.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svcfgsenialcolor_new') }}">Create a new svCfgSenialColor</a>
        </li>
    </ul>
{% endblock %}
