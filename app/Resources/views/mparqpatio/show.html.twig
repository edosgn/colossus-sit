{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mparqpatio</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mparqPatio.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ mparqPatio.nombre }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ mparqPatio.direccion }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if mparqPatio.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('mparqpatio_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('mparqpatio_edit', { 'id': mparqPatio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
