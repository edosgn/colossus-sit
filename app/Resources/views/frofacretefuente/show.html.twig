{% extends 'base.html.twig' %}

{% block body %}
    <h1>Frofacretefuente</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ froFacRetefuente.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if froFacRetefuente.fecha %}{{ froFacRetefuente.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Retencion</th>
                <td>{{ froFacRetefuente.retencion }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if froFacRetefuente.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('frofacretefuente_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('frofacretefuente_edit', { 'id': froFacRetefuente.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
