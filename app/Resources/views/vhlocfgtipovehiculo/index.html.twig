                <td>{{ vhloCfgTipoVehiculo.nombre }}</td>
                <td>{% if vhloCfgTipoVehiculo.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('vhlocfgtipovehiculo_show', { 'id': vhloCfgTipoVehiculo.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('vhlocfgtipovehiculo_edit', { 'id': vhloCfgTipoVehiculo.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlocfgtipovehiculo_new') }}">Create a new vhloCfgTipoVehiculo</a>
        </li>
    </ul>
{% endblock %}
