{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svipatasignacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ svIpatAsignacion.id }}</td>
            </tr>
            <tr>
                <th>Rangoinicial</th>
                <td>{{ svIpatAsignacion.rangoInicial }}</td>
            </tr>
            <tr>
                <th>Rangofinal</th>
                <td>{{ svIpatAsignacion.rangoFinal }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ svIpatAsignacion.total }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if svIpatAsignacion.fecha %}{{ svIpatAsignacion.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Numeroacta</th>
                <td>{{ svIpatAsignacion.numeroActa }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if svIpatAsignacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svipatasignacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('svipatasignacion_edit', { 'id': svIpatAsignacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
