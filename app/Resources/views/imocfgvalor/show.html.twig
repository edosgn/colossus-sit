{% extends 'base.html.twig' %}

{% block body %}
    <h1>Imocfgvalor</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imoCfgValor.id }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ imoCfgValor.valor }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if imoCfgValor.fecha %}{{ imoCfgValor.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imocfgvalor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imocfgvalor_edit', { 'id': imoCfgValor.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
