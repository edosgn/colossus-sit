{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgcda</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgCda.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgCda.nombre }}</td>
            </tr>
            <tr>
                <th>Nit</th>
                <td>{{ cfgCda.nit }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgCda.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgcda_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgcda_edit', { 'id': cfgCda.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
