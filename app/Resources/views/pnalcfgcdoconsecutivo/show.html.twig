{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalcfgcdoconsecutivo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pnalCfgCdoConsecutivo.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ pnalCfgCdoConsecutivo.numero }}</td>
            </tr>
            <tr>
