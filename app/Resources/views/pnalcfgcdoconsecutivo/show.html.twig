                <td>{% if pnalCfgCdoConsecutivo.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalcfgcdoconsecutivo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pnalcfgcdoconsecutivo_edit', { 'id': pnalCfgCdoConsecutivo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
