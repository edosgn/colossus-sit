                <td>{% if pnalCfgCdoConsecutivo.fechaAsignacion %}{{ pnalCfgCdoConsecutivo.fechaAsignacion|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Estado</th>
