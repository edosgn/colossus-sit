{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalcfgtipocontrato</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pnalCfgTipoContrato.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ pnalCfgTipoContrato.nombre }}</td>
            </tr>
            <tr>
                <th>Horarios</th>
                <td>{% if pnalCfgTipoContrato.horarios %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Prorroga</th>
                <td>{% if pnalCfgTipoContrato.prorroga %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if pnalCfgTipoContrato.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalcfgtipocontrato_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pnalcfgtipocontrato_edit', { 'id': pnalCfgTipoContrato.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
