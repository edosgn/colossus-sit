{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vehiculoacreedor</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vehiculoAcreedor.id }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{% if vehiculoAcreedor.estado %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vehiculoacreedor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vehiculoacreedor_edit', { 'id': vehiculoAcreedor.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
