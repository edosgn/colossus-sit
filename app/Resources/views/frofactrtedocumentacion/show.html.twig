{% extends 'base.html.twig' %}

{% block body %}
    <h1>Frofactrtedocumentacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ froFacTrteDocumentacion.id }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ froFacTrteDocumentacion.observacion }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if froFacTrteDocumentacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('frofactrtedocumentacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('frofactrtedocumentacion_edit', { 'id': froFacTrteDocumentacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
