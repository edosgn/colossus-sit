{% extends 'base.html.twig' %}

{% block body %}
    <h1>Usercfgrole</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ userCfgRole.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ userCfgRole.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if userCfgRole.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('usercfgrole_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('usercfgrole_edit', { 'id': userCfgRole.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
