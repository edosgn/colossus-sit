{% extends 'base.html.twig' %}

{% block body %}
    <h1>CfgBodega</h1>

    <table>
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgBodega.nombre }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{% if cfgBodega.estado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ cfgBodega.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgBodega_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgBodega_edit', { 'id': cfgBodega.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
