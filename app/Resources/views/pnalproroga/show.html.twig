{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pnalproroga</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pnalProroga.id }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if pnalProroga.fechaInicio %}{{ pnalProroga.fechaInicio|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{% if pnalProroga.fechaFin %}{{ pnalProroga.fechaFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pnalproroga_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pnalproroga_edit', { 'id': pnalProroga.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
