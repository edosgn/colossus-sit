{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhloacreedor</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vhloAcreedor.id }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if vhloAcreedor.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Gradoalerta</th>
                <td>{{ vhloAcreedor.gradoAlerta }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhloacreedor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vhloacreedor_edit', { 'id': vhloAcreedor.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
