{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mpersonalasignacions list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Desde</th>
                <th>Hasta</th>
                <th>Fechaasignacion</th>
                <th>Rangos</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for mpersonalAsignacion in mpersonalAsignacions %}
            <tr>
                <td><a href="{{ path('mpersonalasignacion_show', { 'id': mpersonalAsignacion.id }) }}">{{ mpersonalAsignacion.id }}</a></td>
                <td>{{ mpersonalAsignacion.desde }}</td>
                <td>{{ mpersonalAsignacion.hasta }}</td>
                <td>{% if mpersonalAsignacion.fechaAsignacion %}{{ mpersonalAsignacion.fechaAsignacion|date('Y-m-d') }}{% endif %}</td>
                <td>{{ mpersonalAsignacion.rangos }}</td>
                <td>{% if mpersonalAsignacion.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('mpersonalasignacion_show', { 'id': mpersonalAsignacion.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('mpersonalasignacion_edit', { 'id': mpersonalAsignacion.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('mpersonalasignacion_new') }}">Create a new mpersonalAsignacion</a>
        </li>
    </ul>
{% endblock %}
