{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svcfgseniallineas list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for svCfgSenialLinea in svCfgSenialLineas %}
            <tr>
                <td><a href="{{ path('svcfgseniallinea_show', { 'id': svCfgSenialLinea.id }) }}">{{ svCfgSenialLinea.id }}</a></td>
                <td>{{ svCfgSenialLinea.nombre }}</td>
                <td>{% if svCfgSenialLinea.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('svcfgseniallinea_show', { 'id': svCfgSenialLinea.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('svcfgseniallinea_edit', { 'id': svCfgSenialLinea.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svcfgseniallinea_new') }}">Create a new svCfgSenialLinea</a>
        </li>
    </ul>
{% endblock %}
