{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cvcdotrazabilidad</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cvCdoTrazabilidad.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if cvCdoTrazabilidad.fecha %}{{ cvCdoTrazabilidad.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cvCdoTrazabilidad.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cvcdotrazabilidad_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cvcdotrazabilidad_edit', { 'id': cvCdoTrazabilidad.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
