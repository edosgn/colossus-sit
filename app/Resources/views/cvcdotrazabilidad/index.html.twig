{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cvcdotrazabilidads list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cvCdoTrazabilidad in cvCdoTrazabilidads %}
            <tr>
                <td><a href="{{ path('cvcdotrazabilidad_show', { 'id': cvCdoTrazabilidad.id }) }}">{{ cvCdoTrazabilidad.id }}</a></td>
                <td>{% if cvCdoTrazabilidad.fecha %}{{ cvCdoTrazabilidad.fecha|date('Y-m-d') }}{% endif %}</td>
                <td>{% if cvCdoTrazabilidad.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('cvcdotrazabilidad_show', { 'id': cvCdoTrazabilidad.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('cvcdotrazabilidad_edit', { 'id': cvCdoTrazabilidad.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cvcdotrazabilidad_new') }}">Create a new cvCdoTrazabilidad</a>
        </li>
    </ul>
{% endblock %}
