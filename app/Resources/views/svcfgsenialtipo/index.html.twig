{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svcfgsenialtipos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for svCfgSenialTipo in svCfgSenialTipos %}
            <tr>
                <td><a href="{{ path('svcfgsenialtipo_show', { 'id': svCfgSenialTipo.id }) }}">{{ svCfgSenialTipo.id }}</a></td>
                <td>{{ svCfgSenialTipo.nombre }}</td>
                <td>{% if svCfgSenialTipo.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('svcfgsenialtipo_show', { 'id': svCfgSenialTipo.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('svcfgsenialtipo_edit', { 'id': svCfgSenialTipo.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svcfgsenialtipo_new') }}">Create a new svCfgSenialTipo</a>
        </li>
    </ul>
{% endblock %}
