{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mgdremitente</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mgdRemitente.id }}</td>
            </tr>
            <tr>
                <th>Primernombre</th>
                <td>{{ mgdRemitente.primerNombre }}</td>
            </tr>
            <tr>
                <th>Segundonombre</th>
                <td>{{ mgdRemitente.segundoNombre }}</td>
            </tr>
            <tr>
                <th>Primerapellido</th>
                <td>{{ mgdRemitente.primerApellido }}</td>
            </tr>
            <tr>
                <th>Segundoapellido</th>
                <td>{{ mgdRemitente.segundoApellido }}</td>
            </tr>
            <tr>
                <th>Identificacion</th>
                <td>{{ mgdRemitente.identificacion }}</td>
            </tr>
            <tr>
                <th>Correoelectronico</th>
                <td>{{ mgdRemitente.correoElectronico }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if mgdRemitente.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('mgdremitente_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('mgdremitente_edit', { 'id': mgdRemitente.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
