{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svcfgparametro</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ svCfgParametro.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ svCfgParametro.nombre }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ svCfgParametro.valor }}</td>
            </tr>
            <tr>
                <th>Numerocriterios</th>
                <td>{{ svCfgParametro.numeroCriterios }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if svCfgParametro.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svcfgparametro_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
