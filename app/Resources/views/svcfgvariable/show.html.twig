{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svcfgvariable</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ svCfgVariable.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ svCfgVariable.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if svCfgVariable.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svcfgvariable_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
