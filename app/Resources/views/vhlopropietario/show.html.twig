{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhlopropietario</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vhloPropietario.id }}</td>
            </tr>
            <tr>
                <th>Licenciatransito</th>
                <td>{{ vhloPropietario.licenciaTransito }}</td>
            </tr>
            <tr>
                <th>Tramites</th>
                <td>{% if vhloPropietario.tramites %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Fechainicial</th>
                <td>{% if vhloPropietario.fechaInicial %}{{ vhloPropietario.fechaInicial|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafinal</th>
                <td>{% if vhloPropietario.fechaFinal %}{{ vhloPropietario.fechaFinal|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{% if vhloPropietario.estado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if vhloPropietario.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlopropietario_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vhlopropietario_edit', { 'id': vhloPropietario.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
