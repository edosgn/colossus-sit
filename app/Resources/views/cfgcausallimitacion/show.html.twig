{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgcausallimitacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgCausalLimitacion.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgCausalLimitacion.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgCausalLimitacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgcausallimitacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgcausallimitacion_edit', { 'id': cfgCausalLimitacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
