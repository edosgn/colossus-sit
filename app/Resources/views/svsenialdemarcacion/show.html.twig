{% extends 'base.html.twig' %}

{% block body %}
    <h1>Svsenialdemarcacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ svSenialDemarcacion.id }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ svSenialDemarcacion.cantidad }}</td>
            </tr>
            <tr>
                <th>Metraje</th>
                <td>{{ svSenialDemarcacion.metraje }}</td>
            </tr>
            <tr>
                <th>Ancholinea</th>
                <td>{{ svSenialDemarcacion.anchoLinea }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ svSenialDemarcacion.total }}</td>
            </tr>
            <tr>
                <th>Tramovial</th>
                <td>{{ svSenialDemarcacion.tramoVial }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if svSenialDemarcacion.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('svsenialdemarcacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('svsenialdemarcacion_edit', { 'id': svSenialDemarcacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
