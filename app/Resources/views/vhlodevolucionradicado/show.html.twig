                <td>{{ vhloDevolucionRadicado.empresaEnvio }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if vhloDevolucionRadicado.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vhlodevolucionradicado_index') }}">Back to the list</a>
