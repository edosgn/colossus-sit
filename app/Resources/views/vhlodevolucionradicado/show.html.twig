{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vhlodevolucionradicado</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vhloDevolucionRadicado.id }}</td>
            </tr>
            <tr>
                <th>Numerolicenciatransito</th>
                <td>{{ vhloDevolucionRadicado.numeroLicenciaTransito }}</td>
