            <tr>
                <th>Fechaingreso</th>
                <td>{% if vhloDevolucionRadicado.fechaIngreso %}{{ vhloDevolucionRadicado.fechaIngreso|date('Y-m-d H:i:s') }}{% endif %}</td>
