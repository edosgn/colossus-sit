{% extends 'base.html.twig' %}

{% block body %}
    <h1>Agentetransitos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Placa</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for agenteTransito in agenteTransitos %}
            <tr>
                <td><a href="{{ path('agentetransito_show', { 'id': agenteTransito.id }) }}">{{ agenteTransito.id }}</a></td>
                <td>{{ agenteTransito.placa }}</td>
                <td>{% if agenteTransito.estado %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('agentetransito_show', { 'id': agenteTransito.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('agentetransito_edit', { 'id': agenteTransito.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('agentetransito_new') }}">Create a new agenteTransito</a>
        </li>
    </ul>
{% endblock %}
