{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgorganismotransito</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgOrganismoTransito.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ cfgOrganismoTransito.nombre }}</td>
            </tr>
            <tr>
                <th>Divipo</th>
                <td>{{ cfgOrganismoTransito.divipo }}</td>
            </tr>
            <tr>
                <th>Asignacionrango</th>
                <td>{% if cfgOrganismoTransito.asignacionRango %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Narinio</th>
                <td>{% if cfgOrganismoTransito.narinio %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgOrganismoTransito.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgorganismotransito_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgorganismotransito_edit', { 'id': cfgOrganismoTransito.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
