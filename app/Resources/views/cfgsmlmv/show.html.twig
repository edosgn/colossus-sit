{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfgsmlmv</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cfgSmlmv.id }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ cfgSmlmv.valor }}</td>
            </tr>
            <tr>
                <th>Anio</th>
                <td>{{ cfgSmlmv.anio }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if cfgSmlmv.fecha %}{{ cfgSmlmv.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if cfgSmlmv.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfgsmlmv_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfgsmlmv_edit', { 'id': cfgSmlmv.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
