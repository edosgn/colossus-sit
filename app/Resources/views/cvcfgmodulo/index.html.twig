{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cvcfgmodulos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Activo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cvCfgModulo in cvCfgModulos %}
            <tr>
                <td><a href="{{ path('cvcfgmodulo_show', { 'id': cvCfgModulo.id }) }}">{{ cvCfgModulo.id }}</a></td>
                <td>{{ cvCfgModulo.nombre }}</td>
                <td>{% if cvCfgModulo.activo %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('cvcfgmodulo_show', { 'id': cvCfgModulo.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('cvcfgmodulo_edit', { 'id': cvCfgModulo.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cvcfgmodulo_new') }}">Create a new cvCfgModulo</a>
        </li>
    </ul>
{% endblock %}
